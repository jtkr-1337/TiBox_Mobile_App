<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".subject_cell_2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/background"
            android:layout_width="348dp"
            android:layout_height="151dp"
            android:background="#C6E3F2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="117dp" />

        <TextView
            android:id="@+id/separator"
            android:layout_width="235dp"
            android:layout_height="2dp"
            android:background="#6099B6"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/background"
            app:layout_constraintEnd_toEndOf="@+id/background"
            app:layout_constraintHorizontal_bias="0.884"
            app:layout_constraintStart_toStartOf="@+id/background"
            app:layout_constraintTop_toTopOf="@+id/background"
            app:layout_constraintVertical_bias="0.496" />

        <TextView
            android:id="@+id/room1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Каб. 208"
            app:layout_constraintBottom_toTopOf="@+id/separator"
            app:layout_constraintEnd_toStartOf="@+id/subject1"
            app:layout_constraintStart_toStartOf="@+id/background"
            app:layout_constraintTop_toTopOf="@+id/background" />

        <TextView
            android:id="@+id/room2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Каб. 201"
            app:layout_constraintBottom_toBottomOf="@+id/background"
            app:layout_constraintEnd_toStartOf="@+id/subject2"
            app:layout_constraintStart_toStartOf="@+id/background"
            app:layout_constraintTop_toBottomOf="@+id/separator" />

        <TextView
            android:id="@+id/time_double"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10:40-12:10"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/separator"
            app:layout_constraintEnd_toStartOf="@+id/separator"
            app:layout_constraintHorizontal_bias="0.583"
            app:layout_constraintStart_toStartOf="@+id/background"
            app:layout_constraintTop_toTopOf="@+id/separator" />

        <TextView
            android:id="@+id/subject1"
            android:layout_width="219dp"
            android:layout_height="41dp"
            android:text="1пг. Проектирование веб ресурсов"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/separator"
            app:layout_constraintEnd_toEndOf="@+id/background"
            app:layout_constraintHorizontal_bias="0.775"
            app:layout_constraintStart_toStartOf="@+id/background"
            app:layout_constraintTop_toTopOf="@+id/background"
            app:layout_constraintVertical_bias="0.147" />

        <TextView
            android:id="@+id/subject2"
            android:layout_width="219dp"
            android:layout_height="44dp"
            android:text="2пг. Пакеты прикладных программ"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/background"
            app:layout_constraintEnd_toEndOf="@+id/background"
            app:layout_constraintHorizontal_bias="0.775"
            app:layout_constraintStart_toStartOf="@+id/background"
            app:layout_constraintTop_toTopOf="@+id/separator"
            app:layout_constraintVertical_bias="0.25" />

        <TextView
            android:id="@+id/name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Кулпеппер Полина Дмитриевна"
            android:textColor="#6E6E6E"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/separator"
            app:layout_constraintEnd_toEndOf="@+id/background"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/subject1"
            app:layout_constraintTop_toBottomOf="@+id/subject1"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/name2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Тихвинский Виталий Игоревич"
            android:textColor="#6E6E6E"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/background"
            app:layout_constraintEnd_toEndOf="@+id/background"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/name1"
            app:layout_constraintTop_toBottomOf="@+id/subject2"
            app:layout_constraintVertical_bias="0.0" />

        <ImageButton
            android:id="@+id/info_button1"
            android:layout_width="32dp"
            android:layout_height="40dp"
            android:background="#00FFFFFF"
            android:src="@drawable/subject_info_button"
            app:layout_constraintBottom_toTopOf="@+id/separator"
            app:layout_constraintEnd_toEndOf="@+id/background"
            app:layout_constraintTop_toTopOf="@+id/background" />

        <ImageButton
            android:id="@+id/info_button2"
            android:layout_width="32dp"
            android:layout_height="40dp"
            android:background="#00FFFFFF"
            android:src="@drawable/subject_info_button"
            app:layout_constraintBottom_toBottomOf="@+id/background"
            app:layout_constraintEnd_toEndOf="@+id/background"
            app:layout_constraintTop_toTopOf="@+id/subject2"
            app:layout_constraintVertical_bias="0.468" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>